From fd2b56e26f2eb3b1682d3df237831351be75944a Mon Sep 17 00:00:00 2001
From: Vyacheslav Bocharov <adeep@lexina.in>
Date: Thu, 13 Jul 2023 14:07:06 +0300
Subject: [PATCH 11/12] Enable emmc on sdcard

---
 arch/arm/boot/dts/sun8i-t113.dtsi | 7 +++++--
 1 file changed, 5 insertions(+), 2 deletions(-)

diff --git a/arch/arm/boot/dts/sun8i-t113.dtsi b/arch/arm/boot/dts/sun8i-t113.dtsi
index affdd2ab5740..a949fce34418 100644
--- a/arch/arm/boot/dts/sun8i-t113.dtsi
+++ b/arch/arm/boot/dts/sun8i-t113.dtsi
@@ -805,7 +805,7 @@ mbus: dram-controller@3102000 {
 			#interconnect-cells = <1>;
 		};
 
-		mmc0: mmc@4020000 {
+		mmc0: mmc@4020000 { /* SD Card */
 			compatible = "allwinner,sun20i-d1-mmc";
 			reg = <0x4020000 0x1000>;
 			interrupts = <GIC_SPI 40 IRQ_TYPE_LEVEL_HIGH>;
@@ -813,9 +813,12 @@ mmc0: mmc@4020000 {
 			clock-names = "ahb", "mmc";
 			resets = <&ccu RST_BUS_MMC0>;
 			reset-names = "ahb";
+			cap-mmc-highspeed;
 			cap-sd-highspeed;
+			mmc-ddr-1_8v;
+			mmc-ddr-3_3v;
 			max-frequency = <150000000>;
-			no-mmc;
+			/* no-mmc; */
 			status = "disabled";
 			#address-cells = <1>;
 			#size-cells = <0>;
-- 
2.34.1

